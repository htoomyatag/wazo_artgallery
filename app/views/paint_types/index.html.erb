<ul class="breadcrumb">
  <li><a href="#" id="a_style">Admin Panel</a> <span class="divider">/</span></li>
    <li><a href="#" id="a_style">Dashboard</a> <span class="divider">/</span></li>
    <li class="active">Types</li>
</ul>
<div class="page-header">
  <h1>Paint Types data<small> list of paint types infomation</small></h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th>Type name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @paint_types.each do |paint_type| %>
      <tr>
        <td><%= paint_type.type_name rescue "-" %></td>
        <td><%= link_to 'Show', paint_type, :id => "a_style" %></td>
        <td><%= link_to 'Edit', edit_paint_type_path(paint_type), :id => "a_style" %></td>
        <td><%= link_to 'Destroy', paint_type, method: :delete, data: { confirm: 'Are you sure?' }, :id => "a_style" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paint type', new_paint_type_path, :class => "btn btn-default" %>

